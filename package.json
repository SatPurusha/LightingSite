{
  "name": "sat-purusha-com",
  "version": "5.0.0",
  "private": true,
  "description": "Lighting by Graham Phoenix",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:sass serve:11ty",
    "build": "npm-run-all --parallel build:sass build:11ty",
    "clean": "rimraf dist",
    "serve:11ty": "eleventy --serve --quiet",
    "build:11ty": "npx eleventy",
    "watch:sass": "npx sass --watch src/sass/style.scss:dist/css/style.css",
    "build:sass": "npx sass src/sass/style.scss:dist/css/style.css"
  },
  "author": "sat purusha",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@types/markdown-it": "^13.0.0",
    "clean-css": "^5.3.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.0",
    "terser": "^5.13.1"
  },
  "devDependencies": {
    "html-minifier-terser": "^7.2.0",
    "rimraf": "^6.0.1"
  }
}
