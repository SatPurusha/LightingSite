{
  "name": "sat-purusha-com",
  "version": "5.0.0",
  "type": "module",
  "private": true,
  "description": "Lighting by Graham Phoenix",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm-run-all clean --parallel watch:*",
    "build": "npm-run-all clean build:*",
    "clean": "rimraf dist",
    "watch:11ty": "eleventy --serve",
    "build:11ty": "eleventy",
    "watch:sass": "sass --watch src/sass:dist/css",
    "build:sass": "sass src/sass:dist/css"
  },
  "author": "sat purusha",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  }
}
